   "source": [
    "#Örnek 2.10 – flatten(), flat(), ravel() fonskiyonları\n",
    "\n",
    "import numpy as np\n",
    "\n",
    "x = np.arange(10).reshape(5,2)\n",
    "print(x)\n",
    "print(\"*******\")\n",
    "print(x.flatten())\n",
    "print(\"*******\")\n",
    "print(x.flat[2])\n",
    "print(\"*******\")\n",
    "print(x.ravel())\n",
    "print(\"*******\")\n",
    "x.ravel()[2]=100\n",
    "print(x)"
   ]
  }
